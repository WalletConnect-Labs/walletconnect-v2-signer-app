{
  "name": "walletconnect-v2-signer-app",
  "description": "Signer App for WalletConnect",
  "private": true,
  "keywords": [
    "wallet",
    "walletconnect",
    "ethereum",
    "jsonrpc",
    "mobile",
    "qrcode",
    "web3",
    "crypto",
    "cryptocurrency",
    "dapp"
  ],
  "author": "WalletConnect Labs <walletconnectlabs.com>",
  "homepage": "https://github.com/walletconnect-labs/walletconnect-v2-monorepo/",
  "license": "LGPL-3.0",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start -- --reset-cache",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "clear": "watchman watch-del-all && rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/metro-* && rm -rf $TMPDIR/haste-map-*",
    "postinstall": "rn-nodeify --hack --install crypto"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.13.4",
    "@walletconnect/client": "^2.0.0-alpha.26",
    "caip-wallet": "^1.6.4",
    "node-libs-react-native": "^1.2.1",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-crypto": "^2.2.0",
    "react-native-randombytes": "^3.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/jest": "^25.2.3",
    "@types/react-native": "^0.63.2",
    "@types/react-test-renderer": "^16.9.2",
    "babel-jest": "^25.1.0",
    "eslint": "^6.5.1",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-test-renderer": "16.13.1",
    "rn-nodeify": "^10.2.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "react-native": {
    "crypto": "react-native-crypto"
  },
  "browser": {
    "crypto": "react-native-crypto"
  }
}
